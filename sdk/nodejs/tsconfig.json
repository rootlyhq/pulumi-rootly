{
    "compilerOptions": {
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "files": [
        "cause.ts",
        "config/index.ts",
        "config/vars.ts",
        "functionality.ts",
        "incidentRole.ts",
        "incidentType.ts",
        "index.ts",
        "provider.ts",
        "service.ts",
        "severity.ts",
        "team.ts",
        "utilities.ts"
    ]
}
